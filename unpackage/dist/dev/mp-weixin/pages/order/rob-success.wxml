<view class="page"><block wx:if="{{loading}}"><our-loading vue-id="5bc416d6-1" active="{{true}}" text="加载中..." bind:__l="__l"></our-loading></block><uni-popup class="vue-ref" vue-id="5bc416d6-2" type="kefu" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><ke-fu-pop class="vue-ref" vue-id="{{('5bc416d6-3')+','+('5bc416d6-2')}}" phone="{{phone}}" data-ref="kefuPop" data-event-opts="{{[['^call',[['call']]]]}}" bind:call="__e" bind:__l="__l"></ke-fu-pop></uni-popup><uni-popup class="vue-ref" vue-id="5bc416d6-4" type="uploadProgress" data-ref="popupU" bind:__l="__l" vue-slots="{{['default']}}"><upload-progress class="vue-ref" bind:submitProgress="__e" vue-id="{{('5bc416d6-5')+','+('5bc416d6-4')}}" data-ref="uploadProgressPop" data-event-opts="{{[['^submitProgress',[['submitProgress']]]]}}" bind:__l="__l"></upload-progress></uni-popup><view class="main"><view class="main-content"><view class="content"><view class="head"><view class="order-status-img"><image src="{{orderStatusControl[orderDetail.orderStatus].statusImg}}"></image></view><view class="order-status-text">{{orderStatusControl[orderDetail.orderStatus].statusText}}</view><view class="order-status-tips">{{orderStatusControl[orderDetail.orderStatus].statusTips}}</view></view><view class="mid-item"><view class="mid-item-title">期待交货时间</view><view class="mid-item-value">{{orderDetail.orderExpectTime}}</view></view><view class="mid-item-product"><view class="mid-item-title">{{orderDetail.serverClassificationName}}</view><view class="product-item"><view class="product-title">数量:</view><view class="product-value">{{orderDetail.orderTotalQuantity}}</view></view><view class="product-item"><view class="product-title">预算金额:</view><view class="product-value">{{orderDetail.orderBudgetAmount+"元"}}</view></view><view class="product-item"><view class="product-title">预付比例:</view><view class="product-value">{{orderDetail.orderBudgetProportion+"%"}}</view></view></view><view class="mid-img-des"><view class="mid-img-title">附件</view><view class="img"><block wx:for="{{orderDetail.pdbWechatOrderItems}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="img-item"><image src="{{item.serverClassificationPic}}" data-event-opts="{{[['tap',[['previewImage',['$0',index],[[['orderDetail.pdbWechatOrderItems','',index,'serverClassificationPic']]]]]]]}}" bindtap="__e"></image></view></block></view><view class="mid-des-title">需求描述</view><view class="mid-des-value">{{orderDetail.orderNote}}</view></view><view class="mid-item-product"><view class="mid-item-title">订单信息</view><view class="product-item"><view class="product-title">创建时间:</view><view class="product-value">{{orderDetail.orderCreateTime}}</view></view></view><block wx:if="{{orderDetail.orderStatus&&orderDetail.orderStatus!=11&&orderDetail.orderStatus!=12}}"><view class="bottom"><block wx:for="{{btns}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['btnsConfig',['$0'],[[['btns','',index,'opt']]]]]]]}}" class="btn-item" bindtap="__e">{{item.optText}}</view></block></view></block></view></view></view></view>