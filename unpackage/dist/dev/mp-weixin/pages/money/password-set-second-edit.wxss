.page {
  height: 100%;
}
.page .main {
  width: 100%;
  background-color: #FFFFFF;
  height: 100%;
}
.page .main .main-content .content {
  padding: 25rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.page .main .main-content .content .title {
  margin-top: 200rpx;
  color: #333333;
  font-size: 32rpx;
  font-weight: 600;
}
.page .main .main-content .content .title-msg {
  margin-top: 20rpx;
  color: #999999;
  font-size: 25rpx;
}
.page .main .main-content .content .password-input {
  width: 95%;
  padding: 10rpx 0rpx;
  margin-top: 40rpx;
}
.page .main .main-content .content .item-bottom {
  width: 100%;
  margin-top: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.page .main .main-content .content .item-bottom .item-submit:active {
  opacity: 0.55;
}
.page .main .main-content .content .item-bottom .item-submit {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 95%;
  height: 80rpx;
  background: #ffc800;
  font-size: 28rpx;
  font-weight: 600;
  color: #333333;
  border-radius: 15rpx;
}
.page .main .main-content .content .item-bottom .item-submit-no {
  background-color: #FFEBA4;
  color: #FFFFFF;
}

